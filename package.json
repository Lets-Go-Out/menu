    "redux-mock-store": "^1.5.3",
    "redux-test-utils": "^0.3.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.1",
    "sinon": "^7.1.0",
    "style": "0.0.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-watch": "^0.2.0"
  }
